{
  "name": "gigamesh-frontend",
  "version": "1.0.0",
  "description": "A personal information manager.",
  "private": true,
  "scripts": {
    "build": "cp -R static/. dist && parcel build src/index.html",
    "check-without-building": "tsc --noEmit && npx eslint './**/*{.js,.jsx,.ts,.tsx}' && prettier --check .",
    "development": "cp -R static/. dist && parcel --hmr-port 1235 --no-autoinstall src/index.html",
    "format": "prettier --write ."
  },
  "author": "Stephan Boyer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "fastclick": "^1.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "autoprefixer": "^9.7.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "typescript": "^3.9.2"
  },
  "postcss": {
    "plugins": [
      "autoprefixer"
    ]
  }
}
