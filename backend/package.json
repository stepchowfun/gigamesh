{
  "name": "gigamesh-backend",
  "version": "1.0.0",
  "description": "A home for all your notes.",
  "private": true,
  "main": "dist/entry/entry.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "check": "tsc --project tsconfig.json && jest --config jest.config.js && eslint --config .eslintrc.js --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0 './**/*{.js,.ts}' && prettier --config .prettierrc.json --ignore-path .gitignore --check .",
    "development": "tsc --project tsconfig.json && NODE_ENV=development PORT=8081 node dist/entry/entry.js",
    "format": "prettier --config .prettierrc.json --ignore-path .gitignore --write ."
  },
  "author": "Stephan Boyer",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.164",
    "@types/pg": "^7.14.5",
    "@types/traverse": "^0.6.32",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.2.0",
    "@sendgrid/mail": "^7.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "pg": "^8.4.1",
    "runtypes": "^5.0.1",
    "traverse": "^0.6.6",
    "winston": "^3.3.3"
  }
}
